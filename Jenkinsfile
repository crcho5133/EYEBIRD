    agent any

    stages {
        stage('Build') {
            steps {
                // 빌드 관련 작업을 여기에 추가
                echo 'Building...'
            }
        }

        stage('Test') {
            steps {
                // 테스트 관련 작업을 여기에 추가
                echo 'Testing...'
            }
        }

        stage('Deploy') {
            steps {
                // 배포 관련 작업을 여기에 추가
                echo 'Deploying...'
            }
        }
    }
}
